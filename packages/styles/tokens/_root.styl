*:root
    --aura: aura
    --aura-font-stack: font-stack
    --aura-font-quotes: font-stack-quote
    --aura-bg: var(--aura-white);
    --aura-text-primary: darken(Snow, 99%);
    --aura-text-primary-inverse: White;
    --aura-accents-3: darken(#f0f0f7, 7%);
    --aura-accents-2: darken(#f0f0f7, 3%);
    --aura-accents-1: darken(#f0f0f7, 1%);
    --aura-accents-0: Snow;
    --aura-module: var(--aura-accents-0);
    --aura-accents-primary: var(--aura-black);
    --aura-accents-secondary: darken(Blue, 60%);
    --aura-accents-success: #045D3C;
    --aura-accents-success-bg: var(--aura-teal-green);
    --aura-accents-info: #0927EC;
    --aura-accents-info-bg: var(--aura-purple);
    --aura-accents-warning: darken(Yellow, 80%);
    --aura-accents-warning-bg: var(--aura-yellow);
    --aura-accents-danger: #8E3106;
    --aura-accents-danger-bg: var(--aura-orange);
    --aura-input-radius: 6.5px;
    --aura-button-radius: 6.5px;
    --aura-input-bg: #f0f0f7;
    --aura-input-placeholder-color: rgba(Black, 0.5);
    --aura-radius: aura
    --aura-opacity: 0.5;
    --aura-outline: #0927EC solid 2px 
    --aura-button-hover: darken(Snow, 80%);
    --aura-link: var(--aura-text-primary);
    --aura-link-hover: var(--aura-accents-1);
    --aura-selector: darken(TealGreen, 5%)
    --aura-loader: darken(TealGreen, 5%)
    --aura-skeleton-start: hsl(200, 20%, 80%)
    --aura-skeleton-end: hsl(200, 20%, 95%)