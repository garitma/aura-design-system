
.videoWrapper,
div[data-oembed-type="video"] 
    position relative
    padding-bottom 56.25%
    padding-top 25px
    height 0
    text-align: center
    overflow: hidden

.videoWrapper iframe,
div[data-oembed-type="video"] iframe
    position absolute
    top 0
    left 0
    width 100%
    height 100%

div[data-type="static_image"]
    @extend .block-img
    img
        width: 100%
